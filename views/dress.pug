extends layout

block content
  h1= title
  -
    let results = [
      {
        "dress_brand": "Jockey",
        "dress_size": "XL",
        "dress_cost": 25
      },
      {
        "dress_brand": "American Eagle",
        "dress_size": "XXL",
        "dress_cost": 35
      },
      {
        "dress_brand": "Nike",
        "dress_size": "XL",
        "dress_cost": 30
      },                   
    ]
  div(class= "container-fluid")
    div(class = "row")
      each item,index in results
        div(class= "col-sm-2") Brand: #{item.dress_brand}
        div(class= "col-sm-2") Size: #{item.dress_size}
        div(class= "col-sm-2") Cost: #{item.dress_cost}